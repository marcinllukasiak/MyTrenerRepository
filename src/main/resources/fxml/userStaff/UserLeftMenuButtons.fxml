<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.userStaff.UserLeftMenuButtonsController">
   <children>
      <ToggleButton mnemonicParsing="false" onAction="#openChangePasswordScreen" prefHeight="50.0" prefWidth="150.0" text="Zmień hasło">
         <toggleGroup>
            <ToggleGroup fx:id="ToggleGroupMenu" />
         </toggleGroup>
      </ToggleButton>
      <ToggleButton mnemonicParsing="false" onAction="#openCalculateScreen" prefHeight="50.0" prefWidth="150.0" text="Wylicz BMR" toggleGroup="$ToggleGroupMenu" />
      <ToggleButton mnemonicParsing="false" onAction="#openMesureScreen" prefHeight="50.0" prefWidth="150.0" text="Dokonaj Pomiarów" toggleGroup="$ToggleGroupMenu" />
      <ToggleButton mnemonicParsing="false" onAction="#openHistoryMesureTables" prefHeight="50.0" prefWidth="150.0" text="Historia Pomiarów" toggleGroup="$ToggleGroupMenu" />
      <ToggleButton mnemonicParsing="false" onAction="#logout" prefHeight="50.0" prefWidth="150.0" text="Wyloguj" toggleGroup="$ToggleGroupMenu" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
